/* 这里的样式只应用于这个自定义组件 */
.inner {
  background-color: white;
  position: fixed;
  z-index: 999;
  width: 100%;
  height: 80px;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  box-shadow: 0 -1px 10px -10px #333;
}

.icon {
  width: 60px;
  height: 60px;
}
